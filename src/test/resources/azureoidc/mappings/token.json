{
  "request": {
    "method": "POST",
    "urlPattern": "/tenant-id/oauth2/v2.0/token"
  },
  "response": {
    "transformers": [
      "token-signer"
    ],
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "transformerParameters": {
      "accessToken": "/azureoidc/access-token.json",
      "idToken": "/azureoidc/id-token.json",
      "privateKey": "/azureoidc/mock-azure-oidc-private-key.json"
    }
  }
}
